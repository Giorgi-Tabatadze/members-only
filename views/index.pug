extends layout

block content
  h1= "Welcome to message board"
  if (!message_list) 
  p Error fetching message
  if (message_list)
    each message in message_list 
      .media
        img.align-self-center.mr-3(src='...' alt='user avatar')
        .media-body
          h5.mt-0 #{message.title}
          p #{message.body}



        
